{
  "name": "cloud-mover",
  "version": "0.0.1",
  "description": "Social Media App - Cloud-Mover",
  "main": "index.js",
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "scripts": {
    "create": "webpack",
    "start": "webpack-dev-server --open",
    "coverage": "NODE_ENV=test jest --coverageReporters=lcov",
    "localcoverage": "jest --coverage  --coverageReporters=lcov"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/estand25/cloud-mover.git"
  },
  "keywords": [],
  "author": "Standley Eugene",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/estand25/cloud-mover/issues"
  },
  "homepage": "https://github.com/estand25/cloud-mover#readme",
  "dependencies": {
    "@ionic/react": "^5.6.3",
    "@ionic/react-router": "^5.6.3",
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "firebase": "^9.0.1",
    "material-ui": "^0.20.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-firebase-hooks": "^3.0.4",
    "react-router-dom": "^5.3.0",
    "reactfire": "^4.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.10.1",
    "@testing-library/react": "^10.2.1",
    "babel-loader": "^8.1.0",
    "coveralls": "^3.1.0",
    "css-loader": "^3.6.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^26.0.1",
    "msw": "^0.19.3",
    "style-loader": "^1.2.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/path/to/dir/",
      "<rootDir>/index.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "collectCoverage": true,
    "coverageReporters": [
      "json",
      "html"
    ]
  }
}
